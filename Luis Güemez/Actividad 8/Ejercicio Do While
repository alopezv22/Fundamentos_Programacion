public class MultiplosDeTres {
    public static void main(String[] args) {
        int numero = 1000; 
        int contador = 0; 

        System.out.println("Números múltiplos de tres entre 1000 y 1500:");

        do {
            if (numero % 3 == 0) {
                System.out.println(numero);
                contador++;
            }
            numero++;
        } while (numero <= 1500);

        System.out.println("Cantidad de números múltiplos de tres: " + contador);
    }
}
